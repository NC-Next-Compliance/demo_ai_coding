# Ollama Configuration
OLLAMA_DOCKER_TAG=latest
OLLAMA_PORT=11434
OLLAMA_GPU_DRIVER=nvidia
OLLAMA_GPU_COUNT=1
OLLAMA_NUM_PARALLEL=4
OLLAMA_MAX_LOADED_MODELS=3

# Open WebUI Configuration
WEBUI_DOCKER_TAG=main
OPEN_WEBUI_PORT=3000
WEBUI_SECRET_KEY=

# Open WebUI URL (for browser integration)
WEBUI_URL=http://localhost:3000

# Database Configuration
DATABASE_ENABLE_SQLITE_WAL=true
DATABASE_POOL_SIZE=10
DATABASE_POOL_MAX_OVERFLOW=20
DATABASE_DEDUPLICATE_INTERVAL=0.5

# RAG (Retrieval-Augmented Generation) Configuration
RAG_EMBEDDING_ENGINE=ollama
RAG_EMBEDDING_MODEL=nomic-embed-text
RAG_EMBEDDING_MODEL_AUTO_UPDATE=true
RAG_RERANKING_MODEL=
RAG_TOP_K=5
RAG_RELEVANCE_THRESHOLD=0.0
ENABLE_RAG_HYBRID_SEARCH=false
ENABLE_RAG_WEB_LOADER_SSL_VERIFICATION=true

# Vector Database Configuration (Qdrant)
VECTOR_DB=qdrant
QDRANT_URI=http://qdrant:6333
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334

# ChromaDB Configuration (alternative to Qdrant - disabled by default)
# VECTOR_DB=chroma
# CHROMA_HTTP_HOST=
# CHROMA_HTTP_PORT=
# CHROMA_HTTP_HEADERS=
# CHROMA_HTTP_SSL=false

# Security Configuration
ENABLE_OAUTH_ROLE_MANAGEMENT=true
ENABLE_OAUTH_PERSISTENT_CONFIG=true
ENABLE_LOGIN_FORM=true

# Performance Configuration
AIOHTTP_CLIENT_TIMEOUT=300
AIOHTTP_CLIENT_TIMEOUT_OPENAI_MODEL_LIST=5

# Logging Configuration
GLOBAL_LOG_LEVEL=INFO
SHOW_ADMIN_DETAILS=true
ENABLE_ADMIN_EXPORT=true

# Optional: Web Search Configuration (disabled by default)
# ENABLE_RAG_WEB_SEARCH=false
# RAG_WEB_SEARCH_ENGINE=
# RAG_WEB_SEARCH_RESULT_COUNT=3
# RAG_WEB_SEARCH_CONCURRENT_REQUESTS=10

# Optional: OpenTelemetry Monitoring (disabled by default)
# ENABLE_OTEL=false
# ENABLE_OTEL_METRICS=false
# OTEL_EXPORTER_OTLP_ENDPOINT=
# OTEL_SERVICE_NAME=open-webui

# Hugging Face Token (for future use with vLLM)
HUGGING_FACE_TOKEN=

# Model Configuration
OLLAMA_MODEL=nomic-embed-text

# Task Model for lightweight operations
TASK_MODEL=
TASK_MODEL_EXTERNAL=

# =============================================================================
# AI PROVIDER API KEYS
# =============================================================================

# OpenAI Configuration (GPT-4, GPT-3.5, etc.)
OPENAI_API_KEY=
OPENAI_API_BASE_URL=https://api.openai.com/v1

# Anthropic Configuration (Claude 3.5 Sonnet, etc.)
ANTHROPIC_API_KEY=
ANTHROPIC_API_BASE_URL=https://api.anthropic.com/v1

# Google Gemini Configuration (Gemini 2.0 Flash Experimental, etc.)
GOOGLE_GEMINI_API_KEY=
GOOGLE_GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai

# Together.ai Configuration (for models like Kimi-K2-Thinking)
TOGETHER_API_KEY=
TOGETHER_API_BASE_URL=https://api.together.xyz/v1